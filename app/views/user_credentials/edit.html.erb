<% content_for :breadcrumb do  %>
  <li class="breadcrumb-item active" aria-current="page">
    Editar Usuario
  </li>
<% end %>

<div class="card">
  <div class="card-body">
    <%= form_for @church_men, url: user_credential_path do |f| %>
      <%= render partial: 'layouts/errors', locals: {errors: @church_men.errors }  %>
      <%= render partial: 'form', locals: {admin: @church_men, f: f} %>
      <%= f.submit("Editar",{class: 'btn btn-primary'})%>
      <%= link_to("Volver", user_credentials_path) %>
    <% end %>
  </div>
</div>